{
  "expo": {
    "name": "ez",
    "slug": "ez",
    "scheme": "ez",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "newArchEnabled": true,
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "package": "com.jsm.aora",
      "infoPlist": {
        "NSCameraUsageDescription": "This app needs access to camera to scan QR codes"
      },
      "bundleIdentifier": "com.anonymous.ez"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "permissions": ["CAMERA", "android.permission.CAMERA"],
      "package": "com.anonymous.ez"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-barcode-scanner",
      {
        "cameraPermission": "Permettre à $(PRODUCT_NAME) d'accéder à votre caméra."
      }
    ]
  }
}
